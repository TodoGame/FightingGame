# Get faculty with id = 1
# Responds with FacultyData
GET {{host}}/faculty?id=1
Authorization: Bearer {{auth_token}}
Content-Type: application/json

###

# get all faculties
# Responds with List<FacultyData>
GET {{host}}/faculties/getAll
Content-Type: application/json
Authorization: Bearer {{auth_token}}

###

# get leading faculty
# Responds with FacultyData
GET {{host}}/leadingFaculty
Content-Type: application/json
Authorization: Bearer {{auth_token}}
